<div class="content col-xs-10 col-xs-offset-1">
  <h1>Weather</h1>
  <div class="frosty col-xs-4 col-xs-offset-4">
    <p id='location'><%= location(@response) %></p>
    <p id='conditions'><%= condition(@response) %></p>
    <p id='temperature'><%= temperature(@response) %></p>
    <p id='wind'><%= wind(@response) %></p>
  </div>
</div>



